<UserControl x:Class="ShortCutDeckDesktop.MVVM.ShortCutProfileEditor.Views.ActionEditors.ActoinPCVirtualKeyPressEditorView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ShortCutDeckDesktop.MVVM.ShortCutProfileEditor.Views.ActionEditors"
             mc:Ignorable="d" 
             d:DesignHeight="400" d:DesignWidth="200">
    <Border Background="Beige" CornerRadius="10">
        <TextBox Margin="10" Height="50" VerticalAlignment="Top" FontSize="30" Text="{Binding KeyCode, Mode=TwoWay}"/>
    </Border>
</UserControl>
